@model IEnumerable<Travel_MVC.Models.DiscoverLeft>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/TravelAdmin/Views/Shared/_Layout1.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Photo)
        </th>
        <th></th>
    </tr>

@foreach (var tp in Model) {
    <tr> 
        <td style=" width:25% ">
            <img class="img-fluid" src="@tp.Photo" />
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=tp.id }) |
        </td>
    </tr>
}

</table>
